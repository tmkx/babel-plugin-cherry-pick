{
  "name": "cherry-pick-import",
  "version": "0.0.0",
  "description": "Cherry-pick imported specified identifiers from a module",
  "author": "Tmk <i@tmk.im>",
  "keywords": [
    "import",
    "export",
    "trim",
    "pick",
    "elimination"
  ],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
